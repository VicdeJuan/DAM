	String name;
	
	public Jugador(Tablero tablero,Posicion pos,String name) {
		this.tablero = tablero;
		this.pos = pos;
		this.name = name;
		log = Logger.getLogger("Jugador");	
	}
	
	@Override
	public Object call() throws Exception {
		// TODO Auto-generated method stub
		return 0;
	}

}
