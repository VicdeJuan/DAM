package ejercicio2;

public class Posicion {
	int x;
	int y;
	
	public Posicion(int x,int y) {
		this.x = x;
		this.y = y;
	}
	
	
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
		
	}
	
	@Override 
	public boolean equals(Object obj) {
		Posicion pos = (Posicion) obj;
		return this.x == pos.getX() && this.y == pos.getY();
	}
	
	   @Override
	    public int hashCode() {
	        return Objects.hash(x, y);
	    }
}
