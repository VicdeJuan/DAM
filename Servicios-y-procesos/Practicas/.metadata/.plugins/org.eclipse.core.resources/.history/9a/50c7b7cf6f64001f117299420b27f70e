package ejercicio2;

public class Posicion {
	int x;
	int y;
	
	public Posicion(int x,int y) {
		this.x = x;
		this.y = y;
	}
	
	
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
		
	}
	
	@Override 
	public boolean equals(Posicion pos) {
		return this.x == pos.getX() && this.y == pos.getY();
	}
}
